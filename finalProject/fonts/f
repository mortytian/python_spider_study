<li class=" j_thread_list clearfix" data-field='{&quot;id&quot;:5979208644,&quot;author_name&quot;:&quot;Yours\u74dc&quot;,&quot;author_nickname&quot;:null,&quot;author_portrait&quot;:&quot;d972596f757273e7939c4e0f&quot;,&quot;first_post_id&quot;:123250179177,&quot;reply_num&quot;:9,&quot;is_bakan&quot;:null,&quot;vid&quot;:&quot;&quot;,&quot;is_good&quot;:null,&quot;is_top&quot;:null,&quot;is_protal&quot;:null,&quot;is_membertop&quot;:null,&quot;is_multi_forum&quot;:null,&quot;frs_tpoint&quot;:null}'  data-tid='5979208644' data-thread-type="0" data-floor='44''>
