<li class=" j_thread_list clearfix" data-field='{&quot;id&quot;:6139184336,&quot;author_name&quot;:&quot;hniujr1004cps&quot;,&quot;author_nickname&quot;:null,&quot;author_portrait&quot;:&quot;eba2686e69756a7231303034637073a427&quot;,&quot;first_post_id&quot;:125733879867,&quot;reply_num&quot;:1,&quot;is_bakan&quot;:null,&quot;vid&quot;:&quot;&quot;,&quot;is_good&quot;:null,&quot;is_top&quot;:null,&quot;is_protal&quot;:null,&quot;is_membertop&quot;:null,&quot;is_multi_forum&quot;:null,&quot;frs_tpoint&quot;:null}'  data-tid='6139184336' data-thread-type="0" data-floor='45''>
